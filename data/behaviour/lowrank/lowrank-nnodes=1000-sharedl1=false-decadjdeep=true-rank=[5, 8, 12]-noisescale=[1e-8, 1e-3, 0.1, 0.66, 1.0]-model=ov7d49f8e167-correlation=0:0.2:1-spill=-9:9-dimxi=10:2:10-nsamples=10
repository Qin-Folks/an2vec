date; PATH=$PATH:/home/slerique/bin OMP_NUM_THREADS=1 JULIA_NUM_THREADS=1 parallel -j90 --bar --header : --env PATH --env JULIA_NUM_THREADS --env OMP_NUM_THREADS 'cd /home/slerique/Code/nw2vec && if test $(( 10 + {spill} )) -gt {dimxi} || test $(( 10 - {spill} )) -gt {dimxi}; then false; fi && OUTFOLDER=data/behaviour/lowrank/nnodes=1000-sharedl1=false-decadjdeep=true-rank={rank}-noisescale={noisescale} && OUTPATH=$OUTFOLDER/ov-correlation={correlation}-spill={spill}-dimxi={dimxi}-sample={sample}.npz && if test -e $OUTPATH; then echo "Already done."; false; fi && mkdir -p $OUTFOLDER && julia julia/an2vec-generative.jl -l 100 -k 10 --p_in 0.25 --p_out 0.01 --correlation {correlation} --featuretype lowrank --flowrank_dim {rank} --flowrank_rank {rank} --flowrank_noisescale {noisescale} --dimxiadj $(( 10 + {spill} )) --dimxifeat $((10 - {spill} )) --overlap $(( 20 - {dimxi} )) --savehistory $OUTPATH --bias false --sharedl1 false --decadjdeep true' ::: noisescale 1e-8 1e-3 0.1 0.66 1.0 ::: rank 5 8 12 ::: correlation $(seq 0 0.2 1) ::: spill $(seq -9 9) ::: dimxi $(seq 10 2 20) ::: sample $(seq 1 10)
